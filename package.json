  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gatsbyjs/gatsby-starter-blog.git"
  },
  "scripts": {
    "add-domain": "echo \"jpedroribeiro.com\" > public/CNAME",
    "build-deploy": "gatsby build && npm run \"add-domain\" && gh-pages -t -d public -b master -r https://github.com/jpedroribeiro/jpedroribeiro.github.io.git",
    "deploy": "gh-pages -d public -b master -r https://github.com/jpedroribeiro/jpedroribeiro.github.io.git",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\""
  }
