<script type="text/javascript">
  window.redactorConfig = {
            plugins: ['table', 'fontcolor', 'fontsize', 'uploadcare', 'layouts'],
            uploadcare: {
        buttonLabel: 'Upload image',
        buttonBefore: 'image',
        buttonIconEnabled: true,
        publicKey: '20a3f15d7fc6831cf2c3',
        crop: '1:1, 16:9, 9:16, 4:3, 3:4, 2:3, 3:2, free',
        tabs: 'all'
      },

    imageResizable: true,
    imagePosition: true,
    dragImageUpload: true,
    callbacks: {
      imageUpload: function (image, json) {
        $(image).attr('image-path', json.path);
      },
      imageUploadError: function (json, xhr) {
        alert("Use Uploadcare service.");
      }
    },
  };

  $(function () {
    $('.editor').each(function () {
      let tempConfig = $.extend(true, {}, window.redactorConfig);
      if ($(this).data('limiter')) {
        tempConfig.plugins.push('limiter');
        tempConfig.limiter = $(this).data('limiter');
      }

      $(this).redactor(tempConfig);
    });

    $(document).on('click', '.redactor-trigger-uploadcare', function () {
      let redactorUploadImage = $('.redactor-focus .re-uploadcare-button ')[0];
      if (redactorUploadImage) {
        redactorUploadImage.click();
      }
    })
  });
</script>
